{
  "name": "@travi/babel-preset",
  "description": "My shareable babel preset",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/babel-preset-travi",
  "bugs": "https://github.com/travi/babel-preset-travi/issues",
  "homepage": "https://npm.im/@travi/babel-preset",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:travis": "travis-lint .travis.yml",
    "test": "run-s lint:*",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "generate:readme": "doctoc README.md --entryprefix '*'",
    "prepack": "run-s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "^2.0.4",
    "@travi/eslint-config": "1.0.31",
    "@travi/eslint-config-mocha": "1.0.7",
    "chai": "^4.2.0",
    "commitlint-config-travi": "1.3.6",
    "cz-conventional-changelog": "3.0.2",
    "doctoc": "1.4.0",
    "husky": "^4.0.2",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.1",
    "rollup": "1.30.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "sinon": "^8.0.0",
    "travis-lint": "^1.0.0"
  },
  "dependencies": {
    "@form8ion/babel-preset": "1.6.11"
  }
}
