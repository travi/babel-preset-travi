{
  "name": "@travi/babel-preset",
  "description": "My shareable babel preset",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/babel-preset-travi",
  "bugs": "https://github.com/travi/babel-preset-travi/issues",
  "homepage": "https://npm.im/@travi/babel-preset",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:travis": "travis lint --no-interactive",
    "test": "run-s lint:*",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "generate:readme": "doctoc README.md --entryprefix '*'",
    "prepack": "run-s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/eslint-config-travi": "^1.6.18",
    "commitlint-config-travi": "1.3.0",
    "cz-conventional-changelog": "2.1.0",
    "doctoc": "1.4.0",
    "husky": "^2.0.0",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3",
    "rollup": "1.15.4",
    "rollup-plugin-auto-external": "^2.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-inline-react-svg": "^1.0.0"
  }
}
