{
  "name": "babel-preset-travi",
  "description": "My shareable babel preset",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/babel-preset-travi",
  "bugs": "https://github.com/travi/babel-preset-travi/issues",
  "homepage": "https://npm.im/babel-preset-travi",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:travis": "travis lint --no-interactive",
    "test": "run-s lint:*",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "generate:readme": "doctoc README.md --entryprefix '*'",
    "prepack": "run-s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/eslint-config-travi": "^1.6.18",
    "commitlint-config-travi": "1.2.31",
    "cz-conventional-changelog": "2.1.0",
    "doctoc": "1.3.1",
    "husky": "^1.1.2",
    "npm-run-all": "4.1.3",
    "rimraf": "2.6.2",
    "rollup": "0.67.0"
  },
  "dependencies": {
    "babel-plugin-inline-react-svg": "^1.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1"
  }
}
