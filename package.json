{
  "name": "@travi/babel-preset",
  "description": "My shareable babel preset",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/babel-preset-travi",
  "bugs": "https://github.com/travi/babel-preset-travi/issues",
  "homepage": "https://npm.im/@travi/babel-preset",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "pretest": "run-s build",
    "test": "npm-run-all --print-label --parallel lint:*",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "generate:md": "remark . --output",
    "prepack": "run-s build",
    "prepare": "husky install",
    "lint:publish": "publint"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@form8ion/babel-preset": "1.6.113"
  },
  "devDependencies": {
    "@travi/any": "2.1.10",
    "@travi/eslint-config": "4.0.4",
    "@travi/eslint-config-mocha": "1.0.20",
    "chai": "4.3.8",
    "commitlint-config-travi": "1.4.35",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "npm-run-all2": "6.0.6",
    "publint": "0.2.2",
    "remark-cli": "11.0.0",
    "remark-preset-lint-travi": "6.0.1",
    "remark-toc": "8.0.1",
    "remark-usage": "10.0.1",
    "rimraf": "5.0.1",
    "rollup": "3.29.2",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "16.0.0"
  }
}
