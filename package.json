{
  "name": "@travi/babel-preset",
  "description": "My shareable babel preset",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/babel-preset-travi",
  "bugs": "https://github.com/travi/babel-preset-travi/issues",
  "homepage": "https://npm.im/@travi/babel-preset",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "test": "run-s lint:*",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "generate:md": "remark . --output",
    "prepack": "run-s build",
    "prepare": "husky install"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@form8ion/babel-preset": "1.6.99"
  },
  "devDependencies": {
    "@travi/any": "2.1.8",
    "@travi/eslint-config": "3.0.28",
    "@travi/eslint-config-mocha": "1.0.19",
    "chai": "4.3.7",
    "commitlint-config-travi": "1.4.29",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "npm-run-all": "4.1.5",
    "remark-cli": "11.0.0",
    "remark-preset-lint-travi": "5.0.0",
    "remark-toc": "8.0.1",
    "remark-usage": "10.0.1",
    "rimraf": "5.0.0",
    "rollup": "3.21.0",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "15.0.4"
  }
}
